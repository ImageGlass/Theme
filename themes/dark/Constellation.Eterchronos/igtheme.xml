<ImageGlass>
    <Theme>
        <Info
            name="Constellation"
            version="1.1"
            author="eterchronos"
            email="lukassprovieri@gmail.com"
            description="A nice and sweet theme"
            type="ImageGlass Theme Configuration"
            compatibility="8.0"
            preview="preview.jpg"
        />
        <main
            accentcolor="#282439" 
            accentdarkcolor="#6b6ad3"
            accentlightcolor="#6b6ad3"
            backcolor="#0a0b0b"
            bottombar=""
            bottombarcolor="#0a0b0b"
            isshowtitlebarlogo="true"
            logo="eterchronos.svg"
            menubackgroundcolor="#0a0b0b"
            menubackgroundhovercolor="#6b6ad3"
            menutextcolor="#c7c8cb"
            menutexthovercolor="#E1E3E6"
            statuscolor="#c7c8cb"
            topbar="example.svg.png"
            topbarcolor="#0a0b0b"
            isdarkmode="True"
        />
        <toolbar_icon
            autosizewindow="autosizewindow.svg"
            autozoom="autozoom.svg"
            back="back.svg"
            checkerboard="checkerboard.svg"
            colorpicker="colorpicker.svg"
            convert="convert.svg"
            crop="crop.svg"
            delete="delete.svg"
            edit="edit.svg"
            fliphorz="fliphorz.svg"
            flipvert="flipvert.svg"
            fullscreen="fullscreen.svg"
            gofirst="gofirst.svg"
            golast="golast.svg"
            gotoimage="gotoimage.svg"
            leftrotate="leftrotate.svg"
            menu="menu.svg"
            next="next.svg"
            open="open.svg"
            print="print.svg"
            refresh="refresh.svg"
            rightrotate="rightrotate.svg"
            scaletofill="scaletofill.svg"
            scaletofit="scaletofit.svg"
            scaletoheight="scaletoheight.svg"
            scaletowidth="scaletowidth.svg"
            slideshow="slideshow.svg"
            thumbnail="thumbnail.svg"
            zoomin="zoomin.svg"
            zoomlock="zoomlock.svg"
            zoomout="zoomout.svg"
            zoomtofit="zoomtofit.svg"
        />
    </Theme>
</ImageGlass>
